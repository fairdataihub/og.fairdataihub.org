  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"./**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"./**/*.+(ts|js|tsx)\" --write",
    "prepare": "husky install",
    "commit": "cz",
    "semantic-release": "semantic-release"
